<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.mapper.ArticleMapper">

	<insert id="postArticle">
		insert into article (title, report, entry_time)
		values (#{title}, #{report}, CURRENT_TIMESTAMP);
	</insert>

		<select id="getArticle" resultType="com.example.model.entity.ArticleEntity">
		select
			*
		from
			article
		where
			id = #{id}
	</select>
</mapper>